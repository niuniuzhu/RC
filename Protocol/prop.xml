<?xml version="1.0" encoding="utf-8"?>

<packet-defs ver="1.0">
	<structs>
		<struct id="0" name="byte">
			<field id="value" type="byte" name="值"/>
		</struct>
		<struct id="1" name="bool">
			<field id="value" type="bool" name="值"/>
		</struct>
		<struct id="2" name="short">
			<field id="value" type="short" name="值"/>
		</struct>
		<struct id="3" name="ushort">
			<field id="value" type="ushort" name="值"/>
		</struct>
		<struct id="4" name="int">
			<field id="value" type="int" name="值"/>
		</struct>
		<struct id="5" name="uint">
			<field id="value" type="uint" name="值"/>
		</struct>
		<struct id="6" name="float">
			<field id="value" type="float" name="值"/>
		</struct>
		<struct id="7" name="long">
			<field id="value" type="long" name="值"/>
		</struct>
		<struct id="8" name="ulong">
			<field id="value" type="ulong" name="值"/>
		</struct>
		<struct id="9" name="double">
			<field id="value" type="double" name="值"/>
		</struct>
		<struct id="10" name="string">
			<field id="value" type="string" name="值"/>
		</struct>
		<struct id="20" name="frame_info">
			<field id="frameId" type="int"/>
			<field id="actions" type="alist" struct="action_info"/>
		</struct>
		<struct id="21" name="action_info">
			<field id="type" type="byte"/>
			<field id="sender" type="string"/>
			<conditions>
				<condition key="type" value="0">
					<field id="x" type="float" name="x"/>
					<field id="y" type="float" name="y"/>
					<field id="z" type="float" name="z"/>
				</condition>
				<condition key="type" value="1">
					<field id="target" type="string"/>
				</condition>
			</conditions>
		</struct>
		<struct id="22" name="keyframe">
			<field id="frame" type="int"/>
		</struct>
		<struct id="30" name="transform">
			<field id="position_x" type="float"/>
			<field id="position_y" type="float"/>
			<field id="position_z" type="float"/>
			<field id="rotation_x" type="float"/>
			<field id="rotation_y" type="float"/>
			<field id="rotation_z" type="float"/>
		</struct>
	</structs>
	<modules>
		<module id="100" key="LV_BATTLE">
			<packet cmd="0" struct="keyframe" key="KEYFRAME"/>
			<packet cmd="1" key="CREATE"/>
			<packet cmd="2" key="DESTROY"/>
			<packet cmd="3" struct="ulong" key="ENTITY_AWAKE"/>
			<packet cmd="4" struct="ulong" key="ENTITY_START"/>
			<packet cmd="5" struct="ulong" key="ENTITY_DESTROY"/>
			<packet cmd="6" struct="transform" key="TRANSFORM"/>
		</module>
		<module id="101" key="BATTLE">
			<packet cmd="1000" struct="frame_info" key="SC_FRAME"/>
		</module>
		<module id="255" key="TEST">
			<packet cmd="0" struct="string" reply="255, 1000" key="CS_RPC"/>
			<packet cmd="1000" struct="string" key="SC_RPC"/>
		</module>
	</modules>
</packet-defs>
